{
  "manifest_version": 3,
  "name": "ANP User Extension",
  "version": "1.0.0",
  "description": "ANP用户的Chrome 扩展，支持自定义 OpenAI 兼容 API。",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}